
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutLoginAdmin.cshtml";
}

<h2>Painel de Controle</h2>
Digite a senha e login

<form action="@ViewBag.LinkSubmitAfilia" id="formAfilia" method="post">
    <label class="col-form-label">
        E-mail/Login
    </label>
    <div class="row">
        <input type="text" id="Email" name="Email" placeholder="e-mail" />
    </div>
    <label class="col-form-label">
        Senha
    </label>
    <div class="row">
        <input type="password" id="Senha" name="Senha" style="width:80px;" placeholder="Digite s senha" />
    </div>
    <span id="rFormRetorno" style="color:#FF0000">@Html.Raw(ViewBag.MensagemRetorno)</span>
    <br />
    <input type="submit" id="btnGravar" value="Login" />
</form>


